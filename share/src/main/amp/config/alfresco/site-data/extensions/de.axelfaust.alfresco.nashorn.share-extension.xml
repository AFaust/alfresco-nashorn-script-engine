<?xml version='1.0' encoding='UTF-8' ?>
<extension>
    <modules>
        <module>
            <id>${project.name} - JavaScript Console (Aikau) - Nashorn Repository Web Script Backend Extension</id>
            <auto-deploy>true</auto-deploy>
            <version>${noSnapshotVersion}</version>

            <customizations>

                <!-- Console Tools framework forced JS Console Aikau page not to use a proper package name + depth -->
                <customization>
                    <targetPackageRoot>de.axelfaust.pages</targetPackageRoot>
                    <sourcePackageRoot>de.axelfaust.nashorn.customizations.jsconsole.repositoryWebScriptBackend</sourcePackageRoot>
                </customization>

            </customizations>
        </module>
    </modules>
</extension>